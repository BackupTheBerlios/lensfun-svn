    <lens>
        <maker>Generic</maker>
        <model>Panoramic 10-100mm f/1.0</model>
        <mount>Generic</mount>
        <cropfactor>1.0</cropfactor>
        <type>panoramic</type>
    </lens>

</lensdatabase>
